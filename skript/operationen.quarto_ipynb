      "source": [
        "# 1 Meile = 1.60934 Kilometer\n",
        "# 1 Gallone = 3.78541 Liter\n",
        "\n",
        "mpg = mtcars.groupby(by = 'cyl')['mpg'].mean()\n",
        "\n",
        "liter_100km = 1 / mpg.mul(1.60934).div(3.78541).div(100)\n",
        "\n",
        "print(liter_100km)"
